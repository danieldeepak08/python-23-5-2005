arr1 = [int(x) for x in input("Enter elements of first array separated by space: ").split()]
arr2 = [int(x) for x in input("Enter elements of second array separated by space: ").split()]

if len(arr1) != len(arr2):
    print("The arrays are not the same.")
else:
    are_same = True
    for i in range(len(arr1)):
        if arr1[i] != arr2[i]:
            are_same = False
            break
    if are_same:
        print("The arrays are the same.")
    else:
        print("The arrays are not the same.")
